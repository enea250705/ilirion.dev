{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "cleanUrls": true,
  "version": 2,
  "routes": [
    {
      "src": "/download",
      "dest": "/IlirionIDE.exe",
      "headers": {
        "Content-Type": "application/octet-stream",
        "Content-Disposition": "attachment; filename=IlirionIDE.exe"
      }
    }
  ]
} 